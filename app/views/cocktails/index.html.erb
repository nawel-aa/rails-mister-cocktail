<div class="container">
  <% @cocktails.each do |cocktail| %>
  <ul class="list-unstyled">
    <li><%= link_to cocktail.name, cocktail_path(cocktail) %></li>
  </ul>
  <% end %>
  
  <div class="row">
    <div class="col col-12 col-lg-6">
      <h1>New cocktail</h1>
      <%= render 'form_cocktail' %>
    </div>
  </div>
</div>